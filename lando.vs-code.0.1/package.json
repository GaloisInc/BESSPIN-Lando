{
    "name": "Lando syntax highlighting",
    "displayName": "Lando",
    "description": "Lando language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lando",
            "aliases": ["Lando", "lando"],
            "extensions": [".lando"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lando",
            "scopeName": "source.lando",
            "path": "./syntaxes/lando.tmLanguage.json"
        }]
    }
}